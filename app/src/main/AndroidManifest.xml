<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uz.gita.geographytest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher2"
        android:label="@string/app_name"
        android:name="uz.gita.geographytest.App.App"
        android:roundIcon="@mipmap/ic_launcher2_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyQuiz">
        <activity
        android:screenOrientation="portrait"
            android:name="uz.gita.geographytest.ui.MenuActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="uz.gita.geographytest.ui.ResultsActivity"
            android:exported="true" />
        <activity
            android:name="uz.gita.geographytest.ui.MainActivity"
            android:exported="true" />
    </application>

</manifest>